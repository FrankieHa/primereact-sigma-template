    "@types/react-dom": "^16.9.0",
    "chart.js": "2.9.4",
    "classnames": "2.2.6",
    "fullcalendar": "5.1.0",
    "history": "^5.0.0",
    "moment": "2.27.0",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "primeflex": "2.0.0-rc.1",
    "primeicons": "4.0.0",
